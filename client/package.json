{
    "name": "speech-client",
    "version": "4.2.1",
    "private": true,
    "description": "Client application for an AI-powered speech therapy platform.",
    "author": "Shawn Ray",
    "license": "MIT",
    "repository": {
      "type": "git"
    },
    "scripts": {
      "start": "concurrently \"react-scripts start\" \"npm run mock-api\"",
      "build": "react-scripts build",
      "test": "react-scripts test --env=jsdom --coverage --watchAll=false",
      "eject": "react-scripts eject",
      "lint": "eslint src/**/*.js",
      "lint:fix": "eslint --fix src/**/*.js",
      "format": "prettier --write 'src/**/*.{js,jsx,json,css,scss}'",
      "precommit": "lint-staged",
      "storybook": "start-storybook -p 6006 -s public",
      "build-storybook": "build-storybook -s public",
      "mock-api": "json-server --watch db.json --port 3001 --delay 500 --middlewares ./mock-api/middlewares.js",
      "cypress:open": "cypress open",
      "cypress:run": "cypress run",
      "analyze": "source-map-explorer 'build/static/js/*.js'",
      "generate:types": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
      "@apollo/client": "^3.7.11",
      "@emotion/react": "^11.10.6",
      "@emotion/styled": "^11.10.6",
      "@mui/icons-material": "^5.11.16",
      "@mui/lab": "^5.0.0-alpha.128",
      "@mui/material": "^5.11.16",
      "@reduxjs/toolkit": "^1.9.3",
      "@testing-library/jest-dom": "^5.16.5",
      "@testing-library/react": "^13.4.0",
      "@testing-library/user-event": "^13.5.0",
      "axios": "^1.3.4",
      "concurrently": "^7.6.0",
      "framer-motion": "^6.5.1",
      "graphql": "^16.6.0",
      "i18next": "^22.4.15",
      "i18next-browser-languagedetector": "^7.0.1",
      "i18next-http-backend": "^2.2.1",
      "immer": "^9.0.18",
      "jsonwebtoken": "^9.0.0",
      "lodash": "^4.17.21",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-helmet-async": "^1.3.0",
      "react-hook-form": "^7.43.9",
      "react-i18next": "^12.2.0",
      "react-redux": "^8.0.5",
      "react-router-dom": "^6.8.2",
      "react-scripts": "5.0.1",
      "react-speech-recognition": "^3.9.0",
      "react-spring": "^9.7.1",
      "recharts": "^2.1.16",
      "socket.io-client": "^4.6.1",
      "styled-components": "^5.3.6",
      "web-vitals": "^2.1.4",
      "yup": "^1.0.2"
    },
    "devDependencies": {
      "@babel/core": "^7.21.0",
      "@storybook/addon-actions": "^6.5.16",
      "@storybook/addon-essentials": "^6.5.16",
      "@storybook/addon-interactions": "^6.5.16",
      "@storybook/addon-links": "^6.5.16",
      "@storybook/builder-webpack5": "^6.5.16",
      "@storybook/manager-webpack5": "^6.5.16",
      "@storybook/node-logger": "^6.5.16",
      "@storybook/preset-create-react-app": "^4.1.2",
      "@storybook/react": "^6.5.16",
      "@storybook/testing-library": "^0.0.13",
      "babel-loader": "^8.3.0",
      "cypress": "^12.1.0",
      "eslint": "^8.36.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-jsx-a11y": "^6.7.1",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react": "^7.32.2",
      "eslint-plugin-react-hooks": "^4.6.0",
      "graphql-codegen-typescript-react-apollo": "^3.3.7",
      "husky": "^8.0.3",
      "json-server": "^0.17.3",
      "lint-staged": "^13.2.0",
      "prettier": "^2.8.7",
      "source-map-explorer": "^2.5.2",
      "storybook-addon-material-ui": "^0.10.0",
      "webpack": "^5.75.0"
    },
    "browserslist": {
      "production": [
        ">0.2%",
        "not dead",
        "not op_mini all"
      ],
      "development": [
        "last 1 chrome version",
        "last 1 firefox version",
        "last 1 safari version"
      ]
    }
  }